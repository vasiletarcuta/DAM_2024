<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IstoricAnalizeActivity">

    <TextView
        android:id="@+id/tvTitluIstoricAnalize"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Istoric Analize"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="30dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="90px"
        android:textAlignment="center"
        android:paddingBottom="30px"
        android:paddingTop="30px"
        android:background="#1d174b"
        android:textColor="@color/white"
        android:textStyle="bold"
        />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="843dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="70dp"
        app:layout_constraintTop_toBottomOf="@id/tvTitluIstoricAnalize"/>


</androidx.constraintlayout.widget.ConstraintLayout>