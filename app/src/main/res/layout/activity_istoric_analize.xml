<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IstoricAnalizeActivity"
    android:background="#d4e6f1">

    <TextView
        android:id="@+id/tvTitluIstoricAnalize"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Istoric Analize"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="30dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="90px"
        android:textAlignment="center"
        android:paddingBottom="30px"
        android:paddingTop="30px"
        android:background="#1d174b"
        android:textColor="@color/white"
        android:textStyle="bold"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabAdaugareAnaliza"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/ic_input_add"
        android:backgroundTint="#9DCFFD"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="20dp"/>


    <ListView
        android:id="@+id/lsvAnalize"
        android:layout_width="match_parent"
        android:layout_height="847dp"
        app:layout_constraintTop_toBottomOf="@id/tvTitluIstoricAnalize"
        tools:layout_editor_absoluteX="1dp" />


</androidx.constraintlayout.widget.ConstraintLayout>